DUK=/usr/bin/duk
QJS=/usr/bin/qjs
MUJS=/usr/bin/mujs
GOJA=/usr/bin/goja
SRC=./src/main.js
run:quickjs duktape mujs goja
quickjs:
	@echo --------------------------------------------------------------------------------
	@echo quickjs:
	@echo --------------------------------------------------------------------------------
	@-$(QJS) --std -I ../lib/requirejs/require.js $(SRC)
duktape:
	@echo --------------------------------------------------------------------------------
	@echo duktape:
	@echo --------------------------------------------------------------------------------
	@-$(DUK) ../lib/requirejs//require.js $(SRC)
mujs:
	@echo --------------------------------------------------------------------------------
	@echo mujs:
	@echo --------------------------------------------------------------------------------
	@-$(MUJS) ../lib/requirejs/require.js $(SRC)
goja:
	@echo --------------------------------------------------------------------------------
	@echo goja:
	@echo --------------------------------------------------------------------------------
	@-$(GOJA) ../lib/requirejs/require.js $(SRC)
