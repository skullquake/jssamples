DUK=/usr/bin/duk
QJS=/usr/bin/qjs
MUJS=/usr/bin/mujs
GOJA=/usr/bin/goja
SRC=./src/a.js
default:quickjs duktape mujs goja
quickjs:
	@echo --------------------------------------------------------------------------------
	@echo quickjs:
	@echo --------------------------------------------------------------------------------
	@$(QJS) --std -I ../lib/parsing/parsing.js $(SRC)
duktape:
	@echo --------------------------------------------------------------------------------
	@echo duktape:
	@echo --------------------------------------------------------------------------------
	@$(DUK) ../lib/parsing/parsing.js $(SRC)
mujs:
	@echo --------------------------------------------------------------------------------
	@echo mujs:
	@echo --------------------------------------------------------------------------------
	@$(MUJS) ../lib/parsing/parsing.js $(SRC)
goja:
	@echo --------------------------------------------------------------------------------
	@echo goja:
	@echo --------------------------------------------------------------------------------
	@$(GOJA) ../lib/parsing/parsing.js $(SRC)
