DUK=/usr/bin/duk
QJS=/usr/bin/qjs
MUJS=/usr/bin/mujs
GOJA=/usr/bin/goja
ELK=/usr/bin/goja
SRC=./src/a.js
run:quickjs duktape mujs goja
#elk
quickjs:
	@echo --------------------------------------------------------------------------------
	@echo quickjs:
	@echo --------------------------------------------------------------------------------
	@$(QJS) --std $(SRC)
duktape:
	@echo --------------------------------------------------------------------------------
	@echo duktape:
	@echo --------------------------------------------------------------------------------
	@$(DUK) $(SRC)
mujs:
	@echo --------------------------------------------------------------------------------
	@echo mujs:
	@echo --------------------------------------------------------------------------------
	@$(MUJS) $(SRC)
goja:
	@echo --------------------------------------------------------------------------------
	@echo goja:
	@echo --------------------------------------------------------------------------------
	@$(GOJA) $(SRC)
elk:
#https://github.com/cesanta/elk
	@echo --------------------------------------------------------------------------------
	@echo elk:
	@echo --------------------------------------------------------------------------------
	@$(ELK) $(SRC)
